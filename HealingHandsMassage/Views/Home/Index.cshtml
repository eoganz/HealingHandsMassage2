@{
    ViewData["Title"] = "Home Page";
}


<div class="mainHeader" align="center">
    <!--<img src="~/Content/FamilyPic.jpeg" class="home-Pic" /> Going to change pic and move to left of intro -->
    <div class="headerText">
        <h1><b>Healing Hands Massage</b></h1>

        <h3><b>A massage with a touch of healing for body, mind and soul</b></h3>
    </div>
</div>

<div class="mainIntro">

    <img class="homePicture" src="~/Images/FamilyPhoto.jpeg" />

    <div class="main-para">
        <p>
            I am an occupational therapist that specilizes in all sorts of massages. These include sports injury, deep tissue, pre/post natal, craniosacral, relaxation, symphatic drainage, and detox. Most massages come with stretching of the muscles as well.

        </p>
    </div>

    <div class="rightEditableBox">


        <p class="boxTitle">Stretches</p>

        <div class="slideshow-container">
            <div class="mySlides">
                <div class="numberText">1/3</div>
                <img src="~/Images/Chin-tucks-exercise.png" style="width:100%" alt="Turning Neck Stretch" />
            </div>

            <div class="mySlides">
                <div class="numberText">2/3</div>
                <img src="~/Images/turning_neck_stretch.png" style="width:100%" alt="Turning Neck Stretch"/>
            </div>

            <div class="mySlides">
                <div class="numberText">1/3</div>
                <img src="~/Images/Chin-tucks-exercise.png" style="width:100%" alt="Chin Retraction Stretch"/>
            </div>

            <a class="prev" onclick="plusSlides(-1)">&#10094</a>
            <a class="next" onclick="plusSlides(1)">&#10095</a>
        </div>

        <br />

        <div style="text-align:center">
            <span class="dot" onclick="currentSlide(1)"></span>
            <span class="dot" onclick="currentSlide(2)"></span>
            <span class="dot" onclick="currentSlide(3)"></span>
        </div>

    </div>
</div>


<script src="~/js/slideshow.js"></script>